<?xml version="1.0" encoding="UTF-8"?>

<?import jfxtras.scene.control.*?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<DialogPane expanded="true" prefHeight="582.0" prefWidth="257.0" xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1">
   <header>
      <FlowPane alignment="CENTER" columnHalignment="CENTER" prefHeight="30.0" prefWidth="200.0">
         <children>
            <Label text="Configuration">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font></Label>
         </children>
      </FlowPane>
   </header>
   <content>
      <BorderPane prefHeight="200.0" prefWidth="300.0">
         <center>
            <GridPane minHeight="500.0" BorderPane.alignment="CENTER">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="75.0" prefHeight="75.0" valignment="CENTER" vgrow="SOMETIMES" />
                <RowConstraints minHeight="75.0" prefHeight="75.0" vgrow="SOMETIMES" />
                <RowConstraints minHeight="75.0" prefHeight="75.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="75.0" prefHeight="75.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label text="Temps min journée :" />
                  <Label text="Temps min repas :" GridPane.rowIndex="1" />
                  <Label text="Entrée min matin : " GridPane.rowIndex="2" />
                  <Label text="Sortie max soir :" GridPane.rowIndex="3" />
                  <LocalTimePicker fx:id="dayTime" GridPane.columnIndex="1" />
                  <LocalTimePicker fx:id="mealTime" GridPane.columnIndex="1" GridPane.rowIndex="1" />
                  <LocalTimePicker fx:id="morningTime" GridPane.columnIndex="1" GridPane.rowIndex="2" />
                  <LocalTimePicker fx:id="eveningTime" GridPane.columnIndex="1" GridPane.rowIndex="3" />
               </children>
            </GridPane>
         </center>
      </BorderPane>
   </content>
   <buttonTypes>
      <ButtonType fx:constant="CANCEL" />
      <ButtonType fx:constant="FINISH" />
   </buttonTypes>
</DialogPane>
